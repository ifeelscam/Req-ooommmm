            await query.message.reply_to_message.delete()
        except: 
            pass
    elif data == "home":
        await query.message.edit_text(
            text=f"👋 Hey {query.from_user.mention}\n\n<b>I'm advance bot of providing videos for @Tamil_Anime_Files Channel!!..</b>",
